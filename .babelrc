{
  "env": {
    "build": {
      "presets": [
        ["@babel/preset-env", {"modules": false}]
      ],
      "only": ["./src/", "./test"],
      "plugins": [
        ["@babel/plugin-transform-react-jsx", {"pragma": "minjsx"}]
      ]
    },
    "test": {
      "presets": ["@babel/preset-env"],
      "only": ["./src/", "./test"],
      "plugins": [
        ["@babel/plugin-transform-react-jsx", {"pragma": "minjsx"}]
      ]
    }
  }
}
